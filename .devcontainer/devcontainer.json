{
  "name": "Go Development Container",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",

  "forwardPorts": [5432, 8080],

  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "golang.go",
        "ms-vscode.go",
        "stamblerre.gocode",
        "golang.golint",
        "ms-vscode.GoTestExplorer",
        "EditorConfig.EditorConfig"
      ]
    }
  }
}
